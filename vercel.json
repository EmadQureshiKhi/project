{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "server.js"
    },
    {
      "src": "/home",
      "dest": "server.js"
    },
    {
      "src": "/cordai",
      "dest": "server.js"
    },
    {
      "src": "/cordchain",
      "dest": "server.js"
    },
    {
      "src": "/templates",
      "dest": "server.js"
    },
    {
      "src": "/agents",
      "dest": "server.js"
    },
    {
      "src": "/styles/(.*)",
      "dest": "public/styles/$1"
    },
    {
      "src": "/cordai/styles/(.*)",
      "dest": "public/cordai/styles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
